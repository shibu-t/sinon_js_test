{
    "framework": "mocha",
    "src_files": [
        "lib/*.js",
        "test/*.test.js",
        "node_modules/expect.js/expect.js"
    ],
    "launchers": {
        "Mocha": {
            "command": "mocha test/*.test.js -R tap",
            "protocol": "tap"
        }
    },
    "launch_in_dev": [
        "Mocha"
    ]
}
